<resources>
    <!-- Base application theme. -->
    <style name="Theme.App" parent="Theme.MaterialComponents.DayNight.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/purple500</item>
        <item name="colorPrimaryDark">@color/purple700</item>
        <item name="colorAccent">@color/teal200</item>
    </style>

    <style name="checkbox">
        <!--波纹的颜色-->
        <!--<item name="android:colorControlHighlight">@color/colorPrimary</item>-->
        <!--选中的颜色-->
        <item name="colorAccent">#00B6A4</item>
        <!--未选中的颜色-->
        <item name="colorControlNormal">#00B6A4</item>
    </style>

    <style name="MdmApn">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
    </style>
    <style name="MdmApn.Title">
        <item name="android:textColor">@android:color/white</item>
    </style>

</resources>